<h1>bucket list countries</h1>

<a href="/bucket_list_cities">bucket list cities</a>

<div>
  <% @country.each do |country| %>

  <div>
    <h3><a href="/city/<%= country.id %>"><%= country.name %></a></h3>
  </div>
  <form action="/country/<%= country.id %>/delete" method="POST"><input type="submit" value="Delete"></form>

  <% end %>
</div>

<a href="/add_country_to_bucket_list">Add new country</a>
